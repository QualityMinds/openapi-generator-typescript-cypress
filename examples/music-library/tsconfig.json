{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es5", "dom"],
    "module": "commonjs",
    "types": ["cypress", "node"],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true
  },
  "include": ["**/*.ts", "cypress.d.ts"]
}
